local SimpleUI = loadstring(game:HttpGet(""))()
local main = SimpleUI("My UI")

local tab1 = main.CreateTab("Tab 1")
local section1 = tab1.CreateSection("First Section")
section1.CreateButton("Click Me", function()
    print("Button clicked!")
end)

local tab2 = main.CreateTab("Tab 2")
local section2 = tab2.CreateSection("Second Section")
section2.CreateButton("Another Button", function()
    print("Second clicked")
end)
