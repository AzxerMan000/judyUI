local SimpleUI = loadstring(game:HttpGet("https://yourdomain.com/SimpleUILib.lua"))()
local ui = SimpleUI("Untitled")

local tab = ui.CreateTab("Main")

local section1 = tab.CreateSection("Left Side")
section1.CreateButton("Left A", function() print("Left A") end)
section1.CreateButton("Left B", function() print("Left B") end)

local section2 = tab.CreateSection("Right Side")
section2.CreateButton("Right X", function() print("Right X") end)
section2.CreateButton("Right Y", function() print("Right Y") end)
